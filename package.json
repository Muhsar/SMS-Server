  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "get-age": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.0",
    "mongoose": "^5.10.7",
    "nodemon": "^2.0.4",
    "socket.io": "^2.3.0"
  }
}
