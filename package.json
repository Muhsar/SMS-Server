{
  "name": "school",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run react-start --prefix client",
    "desktop": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run react-build",
    "dev2": "concurrently \"npm run server\" \"npm run desktop\""
